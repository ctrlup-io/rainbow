{
  "name": "@ctrlup/rainbow-react",
  "version": "2.2.2",
  "private": false,
  "description": "Rainbow React, Ctrl Up Design System",
  "repository": "git@github.com:ctrlup-io/rainbow-react.git",
  "license": "MIT",
  "author": "Valentin Barit <valentin.barit@gmail.com>",
  "type": "module",
  "main": "dist/index.js",
  "source": "src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && parcel build --reporter @parcel/reporter-bundle-analyzer",
    "commit": "git-cz",
    "format": "prettier --write {*,src/**/*}.{js,json,md}",
    "lint": "eslint --cache .",
    "release:dry": "node-env-run --exec 'semantic-release --dry-run'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest src --coverage",
    "test:coverage": "npx live-server coverage/lcov-report",
    "test:watch": "jest src --coverage --watchAll"
  },
  "dependencies": {
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "@parcel/babel-preset-env": "^2.7.0",
    "@parcel/reporter-bundle-analyzer": "^2.2.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.19",
    "babel-loader": "^8.2.5",
    "eslint": "^8.24.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-storybook": "^0.6.4",
    "git-cz": "^4.9.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "node-env-run": "^4.0.2",
    "parcel": "^2.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.5",
    "sort-package-json": "^1.57.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.6",
    "react": "^17.0.2 || ^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
